{
  "name": "berlioz/doc-parser",
  "description": "Berlioz Doc Parser is a PHP library to parse documentation and allow to integrate this on website for example.",
  "license": "MIT",
  "homepage": "https://getberlioz.com",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan@getberlioz.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\DocParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\DocParser\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-mbstring": "*",
    "berlioz/html-selector": "^1.0",
    "berlioz/http-message": "^1.0 || 2.x-dev",
    "league/flysystem": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "gregwar/rst": "^1.0",
    "league/commonmark": "^1.0",
    "league/flysystem-memory": "^2.0"
  },
  "suggest": {
    "gregwar/rst": "If you need to generate reStructuredText file format",
    "league/commonmark": "If you need to generate MarkDown file format"
  }
}
