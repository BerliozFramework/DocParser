{
  "name": "berlioz/doc-parser",
  "description": "Berlioz Documentation Parser",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "homepage": "http://www.berlioz-framework.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan.giron@berlioz-framework.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\DocParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\DocParser\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-fileinfo": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "berlioz/html-selector": "@dev",
    "berlioz/http-message": "@dev",
    "erusev/parsedown-extra": "~0.7",
    "gregwar/rst": "~1.0",
    "php-http/httplug": "~1.1.0",
    "psr/simple-cache": "~1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.3"
  }
}
